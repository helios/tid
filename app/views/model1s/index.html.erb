<div class="col-md-8">
<table id="myTable" class="table table-striped"><thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

 <tbody>
    <% @model1s.each do |model1| %>
      <tr>
        <td><%= model1.name %>
            <% if defined?(names_path) %>
            <%= link_to "view", :controller => "names", :query_names => model1.name %>
            <% end %>
        </td>
        <td><%= model1.surname %>
            <% if defined?(surnames_path) %>
            <%= link_to "view", :controller => "surnames", :query_surnames => model1.surname %>
            <% end %>
        </td>
        <td><%= link_to 'Show', model1 %></td>
        <td><%= link_to 'Edit', edit_model1_path(model1) %></td>
        <td><%= link_to 'Destroy', model1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

